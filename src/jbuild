(jbuild_version 1)

(library
 ((name react_ios) 
  (public_name react_ios)))

(rule
 ((targets (react_ios.complete.o))
  (deps (
         react_ios__React.cmx 
         react_ios__Main.cmx
         react_ios__Test_ios.cmx
         ))
  (action (run ocamlfind -toolchain ios ocamlopt -output-complete-obj ${!^} -o react_ios.complete.o))))

(install (
  (package react_ios)
  (section lib)
  (files (react_ios.complete.o))))


